    <string name="ability_poison_cloud">Nube de Veneno</string>
    <string name="ability_poison_cloud_desc">"Libera una nube tóxica, envenenando a todos los enemigos en el área e infligiendo daño continuo."</string>
    <string name="ability_fire_embers">Brasas Ardientes</string>
    <string name="ability_fire_embers_desc">"Esparce brasas ardientes sobre un área amplia, infligiendo daño leve por fuego a todos los enemigos."</string>
    <string name="ability_fire_blaze">Llamarada</string>
    <string name="ability_fire_blaze_desc">"Desata una llama rugiente, quemando a todos los enemigos en el área con daño moderado por fuego."</string>
    <string name="ability_shields_up">¡Escudos Arriba!</string>
    <string name="ability_shields_up_desc">"Aumenta las defensas de tu equipo con un escudo poderoso, reduciendo el daño recibido por un tiempo corto."</string>
    <string name="ability_healing_mushrooms">Hongos Curativos</string>
    <string name="ability_healing_mushrooms_desc">"Haz brotar hongos mágicos que curan gradualmente a tu equipo cuando se consumen."</string>
    <string name="ability_hose_spray">Rociador de Agua</string>
    <string name="ability_hose_spray_desc">"Desata un chorro de agua, empapando al equipo enemigo e infligiendo un daño modesto."</string>
    <string name="ability_slime_spray">Rociador de Baba</string>
    <string name="ability_slime_spray_desc">"Cubre al equipo enemigo con baba pegajosa, ralentizando sus movimientos e infligiendo daño menor."</string>
    <string name="ability_call_mosquito">Llamar Mosquito</string>
